{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "main.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/checkdate",
      "dest": "/api/checkdate.js",
      "methods": ["GET", "POST", "DELETE", "PUT"]
    },
    {
      "src": "/api/checkin/(.*)",
      "dest": "/api/checkin.js",
      "methods": ["GET", "POST", "DELETE", "PUT"]
    }
  ]
}
